	<subDataset name="Table Dataset 1" />
	<queryString>
		<![CDATA[SELECT
	COALESCE(T1.device_id,'') as device_id,
	COALESCE(T1.platform,'') as platform,
	COALESCE(T1.device_os_version,'') as device_os_version,
	COALESCE(T1.device_type,'') as device_type,
	COALESCE(T3.username,'Not mapped yet') as  username
FROM trx_user_device T1
	LEFT JOIN (SELECT * FROM trx_user_device_mapping
				WHERE DEFAULT_DEVICE = TRUE)
	T2 ON T1.id = T2.user_device_id
	LEFT JOIN core_user T3 ON T2.user_id = T3.id]]>
	</queryString>
