	<queryString>
		<![CDATA[SELECT
	COALESCE(device_id,'') as device_id,
	COALESCE(device_platform,'') as device_platform,
	COALESCE(device_os_version,'') as device_os_version,
	COALESCE(device_type,'') as device_type,
	COALESCE(log_menu,'') as log_menu,
	recorded_date
FROM log_application WHERE log_menu is NOT NULL
AND recorded_date BETWEEN CAST($P{FROM} as DATE) AND CAST($P{UNTIL} as DATE) + interval '1 day' - interval '1 second'
ORDER BY recorded_date DESC, device_id ASC, log_menu ASC]]>
	</queryString>
