	<queryString>
		<![CDATA[SELECT
	time_info,
	actor,
	 right(object_name , position('.' in reverse(object_name))-1) as object,
	field_name,
	 value_before,
	 value_after,
	action
FROM audit_trail
WHERE (time_info BETWEEN CAST($P{FROM} as DATE) AND CAST($P{UNTIL} as DATE) + interval '1 day' - interval '1 second')
